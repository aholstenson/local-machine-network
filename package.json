{
  "name": "local-machine-network",
  "version": "0.3.0",
  "license": "MIT",
  "description": "Create a machine local network with leader election",
  "repository": "aholstenson/local-machine-network",
  "keywords": [
    "ipc",
    "socket",
    "leader-election"
  ],
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "ci": "npm run run lint",
